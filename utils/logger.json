  {
	"version": 1,
	"formatters": {
		"simple": {
			"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "simple",
			"stream": "ext://sys.stdout"
		},
		"logfile": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "WARN",
			"formatter": "simple",
			"filename": "tubular.log",
			"maxBytes": 10485760,
			"backupCount": 5,
			"encoding": "utf8"
		}
	},
	"loggers": {
		"tubular": {
			"level": "DEBUG",
			"handlers": [
				"logfile"
			],
			"propagate": "no"
		}
	},
	"root": {
		"level": "INFO",
		"handlers": [
			"console"
		]
	}
}